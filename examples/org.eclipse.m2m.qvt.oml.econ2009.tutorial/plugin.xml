<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.m2m.qvt.oml.econ2009.tutorial"
         point="org.eclipse.ui.newWizards">
