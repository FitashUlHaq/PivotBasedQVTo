transformation uml14to20;

modeltype UML uses 'http://www.borland.com/together/uml';
modeltype UML14 uses 'http://www.borland.com/together/uml14';
modeltype UML20 uses 'http://www.borland.com/together/uml20';

mapping main(in model: uml::together::Model): uml::together::Model {
	init {
	    var allClasses: Sequence(uml14::kernel::classes::Class) := model.ownedMembers->select(c|c.oclIsKindOf(uml14::kernel::classes::Class))->asSequence()->oclAsType(Sequence(uml14::kernel::classes::Class));
	}
    	ownedMembers := allClasses->collect(c|make20Class(c))->asOrderedSet();
}

mapping make20Class(in c: uml14::kernel::classes::Class): uml20::classes::Class {
	    name := c.name + '20';
}
