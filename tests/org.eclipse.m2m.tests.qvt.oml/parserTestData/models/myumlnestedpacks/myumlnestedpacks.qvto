transformation myumlnestedpacks.myumlnestedpacks;

metamodel 'http://www.eclipse.org/emf/2002/Ecore';
metamodel 'http://www.borland.com/together/uml';

mapping main(in model: ecore::EPackage): uml::together::Model  {
    object {
	    nestedPackages += object uml::kernel::packages::Package {
					    	name:='p1';
					        nestedPackages += object uml::kernel::packages::Package { name:='p2' }
					     } 
    }
}
