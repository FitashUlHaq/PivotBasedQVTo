transformation primtypes;

metamodel 'http://www.borland.com/together/uml';
metamodel 'http://www.borland.com/together/uml20';

mapping main(in model: ecore::EPackage): uml::together::Model {
	init {
		var root := object uml20::together::PrimitiveTypesRoot {};
		var treal := root.getPrimitiveType(uml::kernel::PrimitiveTypeEnum::REAL);
	}
	
    	ownedMembers += object uml20::classes::Class {
    		name := 'Employee';
			attributes += object uml20::kernel::Property {
				name := 'name';
				type := treal;
			};    		
			attributes += object uml20::kernel::Property {
				name := 'str';
			    type := (object uml20::together::PrimitiveTypesRoot{}).getPrimitiveType(uml::kernel::PrimitiveTypeEnum::STRING);
			};    		

    	};
}
