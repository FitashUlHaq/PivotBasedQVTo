modeltype UML uses "http://www.eclipse.org/uml2/2.1.0/UML";
modeltype ECORE uses "http://www.eclipse.org/emf/2002/Ecore";
modeltype RDB uses 'http://www.eclipse.org/qvt/1.0.0/Operational/examples/rdb';

transformation exec1(in model1 : ECORE, in model2 : ECORE, out uml : UML, out rdb : RDB);

main(in p1 : EPackage@model1) {

	var umlPackage1 := object UML::Package {name := 'uml1'};
	umlPackage1.packagedElement := p1.eClassifiers->select(c|c.oclIsKindOf(EClass))
		->collect(c|c.oclAsType(EClass).toClass1())->asSet();

	var umlPackage2 := object UML::Package {name := 'uml2'};
	umlPackage2.packagedElement += p1.eClassifiers->select(c|c.oclIsKindOf(EClass))
		->collect(c|c.oclAsType(EClass).toClass2())->asSet();
		
	object rdb::Table {name := 'table'};
}

mapping EClass::toClass1() : Class { 
	name := self.name + '_Cls1';
}

mapping EClass::toClass2() : Class { 
	name := self.name + '_Cls2';
}

